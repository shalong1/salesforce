    <webLinks>
        <fullName>File_Maintenance_Page</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>File Maintenance Page</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)} 
var prelim = window.location.href; 
var prelimregex = new RegExp(&quot;[a-zA-Z0-9]{15,}(?=$|\\?)&quot;);
var fileId = prelimregex.exec(prelim); 
var settings = &quot;{!TestContract__c.Settings__c}&quot;; 
var redirectURL = sforce.apex.execute(&quot;Sertifi2_0/Redirector&quot;,&quot;redirectToFileMaintenanceWithSettings&quot;, {fileId: fileId,settingsString:settings}); 
//window.location = redirectURL; 
window.open(redirectURL);</url>
